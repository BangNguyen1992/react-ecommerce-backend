    "graphql": "^14.6.0",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "nodemailer": "^6.4.10",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prisma": "^1.34.10",
    "prisma-binding": "^2.3.16",
    "stripe": "^8.64.0"
  },
  "license": "MIT",
  "repository": {
    "url": "wesbos.com"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
